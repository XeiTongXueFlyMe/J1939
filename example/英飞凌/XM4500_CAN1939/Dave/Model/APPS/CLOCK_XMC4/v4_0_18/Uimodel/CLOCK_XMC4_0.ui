		<p1:GTab visible="true" enabled="true" text="Clock Generation Settings" widgetName="gtab_clockgeneration">
			<p1:GTab.control>
				<p1:Tab>
					<p1:Tab.layout>
						<GridLayout/>
					</p1:Tab.layout>
					<p1:GGroup text="High Precision Oscillator Settings" widgetName="ggroup_extclk" manifestObj="true">
						<p1:GGroup.layoutData>
							<GridData horizontalAlignment="FILL"/>
						</p1:GGroup.layoutData>
						<p1:GGroup.layout>
							<GridLayout numColumns="2"/>
						</p1:GGroup.layout>
						<p1:GLabel manifestObj="true" text="Operating mode:" widgetName="glabel_extclkmode">
							<p1:GLabel.layoutData>
								<GridData horizontalAlignment="LEFT" widthHint="200"/>
							</p1:GLabel.layoutData>
						</p1:GLabel>
						<p1:GCombo mandatory="(com.ifx.davex.ui.controls.util.AppUiConstants).FALSE" manifestObj="true" options="Disabled&#13;&#10;External Crystal Mode&#13;&#10;External Clock Input Mode" toolTipText="Select a source of external input to the PLL." value="External Crystal Mode" widgetName="gcombo_extclkmode" description="High precision oscillator derived from the following sources:&#13;&#10;&lt;OL&gt;&#13;&#10;&lt;LI&gt;External Crystal Mode: Used to generate a high precision clock via XTAL1 and XTAL2 pins.&lt;/LI&gt;&#13;&#10;&lt;LI&gt;External Clock Input Mode: An external clock is being supplied to Clock generation unit (CGU) from CLKIN pin, via the OSC_HP module, bypassing the oscillator circuit.&lt;/LI&gt;&#13;&#10;&lt;LI&gt;Disabled: High precision oscillator settings are disabled. Only Internal Fast Oscillator can be used to generate system and peripheral clocks.&lt;/LI&gt;&#13;&#10;&lt;/OL&gt;">
							<p1:GCombo.layoutData>
								<GridData grabExcessHorizontalSpace="true" horizontalAlignment="LEFT" widthHint="170"/>
							</p1:GCombo.layoutData>
						</p1:GCombo>
						<p1:GLabel text="External clock frequency [MHz]:" widgetName="glabel_extclkoscfreq" manifestObj="true">
							<p1:GLabel.layoutData>
								<GridData horizontalAlignment="LEFT" widthHint="177"/>
							</p1:GLabel.layoutData>
						</p1:GLabel>
						<p1:GFloat description="The external clock using crystal oscillator or CLKIN pin is a source of main-PLL &amp; USBPLL clocks." format="*,2" mandatory="(com.ifx.davex.ui.controls.util.AppUiConstants).FALSE" manifestObj="true" maxValue="25.0" minValue="4.0" toolTipText="Desired external clock frequency." value="12" widgetName="gfloat_extclkoscfreq" x:Style="BORDER">
							<p1:GFloat.layoutData>
								<GridData grabExcessHorizontalSpace="true" horizontalAlignment="LEFT" widthHint="50"/>
							</p1:GFloat.layoutData>
						</p1:GFloat>
					</p1:GGroup>
					<p1:GGroup text="Internal Fast Oscillator Settings" widgetName="ggroup_fofi">
						<p1:GGroup.layoutData>
							<GridData horizontalAlignment="FILL"/>
						</p1:GGroup.layoutData>
						<p1:GGroup.layout>
							<GridLayout numColumns="2"/>
						</p1:GGroup.layout>
						<p1:GLabel text="Calibration:" widgetName="glabel_backupclktrim">
							<p1:GLabel.layoutData>
								<GridData horizontalAlignment="RIGHT" widthHint="200"/>
							</p1:GLabel.layoutData>
						</p1:GLabel>
						<p1:GCombo description="The internal fast oscillator options are used to calibrate the backup clock by improving the accuracy. Following options are available:&#13;&#10;&lt;OL&gt;&lt;LI&gt;Factory Calibration: The factory calibration enabled with user software via bit field FOTR of the PLLCON0 register.&lt;/LI&gt;&#13;&#10;&lt;LI&gt;Automatic Calibration Using Standby Clock: Backup Clock adjustment based on the  high precision reference clock fSTDBY. The automatic calibration enabled with user software via bit field AOTREN of the PLLCON0 register.&lt;/LI&gt;&#13;&#10;&lt;/OL&gt;" manifestObj="true" options="Factory Calibration&#13;&#10;Automatic Calibration Using Standby Clock" toolTipText="Select a backup clock (fOFI) trimming option.&#13;&#10;" value="Factory Calibration" widgetName="gcombo_backupclktrim">
							<p1:GCombo.layoutData>
								<GridData grabExcessHorizontalSpace="true" horizontalAlignment="LEFT" widthHint="255"/>
							</p1:GCombo.layoutData>
						</p1:GCombo>
					</p1:GGroup>
					<p1:GGroup text="Main PLL Settings" widgetName="ggroup_mainpllset">
						<p1:GGroup.layoutData>
							<GridData horizontalAlignment="FILL"/>
						</p1:GGroup.layoutData>
						<p1:GGroup.layout>
							<GridLayout numColumns="4"/>
						</p1:GGroup.layout>
						<p1:GLabel text="PLL clock source:" widgetName="glabel_pllclksrc"/>
						<p1:GCombo description="The main PLL converts a low-frequency external clock signal into a high-speed internal&#13;&#10;clock. And the main PLL clock is a main source of system frequency (fSYS) and it is derived from following sources:&#13;&#10;&lt;OL&gt;&#13;&#10;&lt;LI&gt;Internal Fast Oscillator: Slow internal 24MHz clock. Used during normal or low power operations after the startup.&lt;/LI&gt;&#13;&#10;&lt;LI&gt;External Crystal High Precision Oscillator: Used to generate a high precision clock via XTAL1 and XTAL2 pins. Alternatively, an external clock is being supplied to clock generation unit (CGU) from CLKIN pin, via the OSC_HP module, bypassing the oscillator circuit.&lt;/LI&gt;&lt;/OL&gt;" manifestObj="true" options="Internal Fast Oscillator&#13;&#10;External Crystal High Precision Oscillator" toolTipText="Select a source of main PLL clock." value="External Crystal High Precision Oscillator" widgetName="gcombo_pllclksrc">
							<p1:GCombo.layoutData>
								<GridData grabExcessHorizontalSpace="true" horizontalAlignment="LEFT" widthHint="255" horizontalSpan="3"/>
							</p1:GCombo.layoutData>
						</p1:GCombo>
						<p1:GLabel text="PLL operating mode:" widgetName="glabel_mainpllset">
							<p1:GLabel.layoutData>
								<GridData widthHint="177" horizontalAlignment="FILL"/>
							</p1:GLabel.layoutData>
						</p1:GLabel>
						<p1:GCombo description="The operating mode selection decides to bypass a VCO or not:&#13;&#10;&lt;OL&gt;&lt;LI&gt; Normal Mode: fPLL is a scaled version of VCO Clock&lt;BR&gt;&#13;&#10;   &#9;&#9;&#9;&#9;  &lt;B&gt;fPLL = (N * fSOC) / (P * K2)&lt;/LI&gt;&lt;/B&gt;&#13;&#10;&lt;LI&gt;Prescaler Mode: fPLL is running at external frequency fOSC (bypassing a VCO)&lt;BR&gt;&#13;&#10;&#9;&#9;&#9;&#9; &lt;B&gt;fPLL = fOSC / K1&lt;/B&gt;&lt;/LI&gt;&lt;/OL&gt;" manifestObj="true" options="Normal Mode&#13;&#10;Prescaler Mode" toolTipText="Select a desired mode to bypass a VCO or not." value="Normal Mode" widgetName="gcombo_mainpllset">
							<p1:GCombo.layoutData>
								<GridData grabExcessHorizontalSpace="true" horizontalAlignment="LEFT" widthHint="255" horizontalSpan="3"/>
							</p1:GCombo.layoutData>
						</p1:GCombo>
						<p1:GLabel manifestObj="true" text="Requested PLL frequency [MHz]:" widgetName="glabel_mainplldesfreq">
							<p1:GLabel.layoutData>
								<GridData widthHint="200" horizontalAlignment="FILL"/>
							</p1:GLabel.layoutData>
						</p1:GLabel>
						<p1:GFloat description="The requested PLL frequency internally configures the PLL dividers: &#13;&#10;&lt;B&gt;NDIV, PDIV, K2DIV and K1DIV:&lt;/B&gt;&#13;&#10;&lt;OL&gt;&lt;LI&gt;In Normal Mode: fPLL is a scaled version of VCO clock&lt;BR&gt;&#13;&#10;   &#9;&#9;&#9;&#9;&#9;&lt;B&gt;fPLL = (N * fSOC) / (P * K2) &lt;/B&gt;&lt;/LI&gt;&#13;&#10;&lt;LI&gt;In Prescaler Mode: fPLL is running at external frequency fOSC (bypassing a VCO)&lt;BR&gt;&#13;&#10;   &#9;&#9;&#9;&#9;&#9;&lt;B&gt;fPLL = fOSC / K1&lt;/B&gt;&lt;/LI&gt;&lt;/OL&gt;" format="*,6" mandatory="(com.ifx.davex.ui.controls.util.AppUiConstants).FALSE" manifestObj="true" maxValue="520.0" minValue="3.0" toolTipText="The requested PLL frequency internally configures the PLL dividers: NDIV, PDIV, K2DIV and K1DIV." value="120" widgetName="gfloat_mainplldesfreq" x:Style="BORDER">
							<p1:GFloat.layoutData>
								<GridData horizontalAlignment="LEFT" widthHint="50"/>
							</p1:GFloat.layoutData>
						</p1:GFloat>
						<p1:GLabel manifestObj="true" text="Actual PLL frequency [MHz]:" widgetName="glabel_mainpllactfreq">
							<p1:GLabel.layoutData>
								<GridData horizontalAlignment="LEFT" widthHint="180"/>
							</p1:GLabel.layoutData>
						</p1:GLabel>
						<p1:GFloat description="The actual PLL frequency internally configures the PLL dividers: &#13;&#10;&lt;B&gt;NDIV, PDIV, K2DIV and K1DIV:&lt;/B&gt;&#13;&#10;&lt;OL&gt;&lt;LI&gt;In Normal Mode: fPLL is a scaled version of VCO Clock&lt;BR&gt;&#13;&#10;   &#9;&#9;&#9;&#9;&#9;&lt;B&gt;fPLL = (N * fSOC) / (P * K2) &lt;/B&gt;&lt;/LI&gt;&#13;&#10;&lt;LI&gt;In Prescaler Mode: fPLL is running at external frequency fOSC (bypassing a VCO)&lt;BR&gt;&#13;&#10;   &#9;&#9;&#9;&#9;&#9;&lt;B&gt;fPLL = fOSC / K1&lt;/B&gt;&lt;/LI&gt;&lt;/OL&gt;" enabled="false" format="*,6" mandatory="(com.ifx.davex.ui.controls.util.AppUiConstants).FALSE" manifestObj="true" maxValue="520.0" minValue="3.0" value="120" widgetName="gfloat_mainpllactfreq" x:Style="BORDER">
							<p1:GFloat.layoutData>
								<GridData horizontalAlignment="LEFT" widthHint="50" grabExcessHorizontalSpace="true"/>
							</p1:GFloat.layoutData>
						</p1:GFloat>
					</p1:GGroup>
					<p1:GGroup manifestObj="true" text="USB PLL Settings" widgetName="ggroup_usb_pll_settings">
						<p1:GGroup.layoutData>
							<GridData horizontalAlignment="FILL"/>
						</p1:GGroup.layoutData>
						<p1:GGroup.layout>
							<GridLayout numColumns="4"/>
						</p1:GGroup.layout>
						<p1:GLabel manifestObj="true" text="Requested USB PLL frequency [MHz]:" widgetName="glabel_usbplldesfreq">
							<p1:GLabel.layoutData>
								<GridData widthHint="200" horizontalAlignment="RIGHT"/>
							</p1:GLabel.layoutData>
						</p1:GLabel>
						<p1:GFloat description="The requested USB PLL frequency internally configures the USB PLL dividers: &#13;&#10;&lt;B&gt;NDIV and PDIV:&lt;/B&gt;&#13;&#10;&lt;OL&gt;&lt;LI&gt;In Normal Mode: fUSBPLL is a scaled version of VCO Clock&lt;BR&gt;&#13;&#10;   &#9;&#9;&#9;&#9;&#9;&lt;B&gt;fUSBPLL  = (N * fSOC) / (P * 2) &lt;/B&gt;&lt;/LI&gt;" format="*,6" mandatory="(com.ifx.davex.ui.controls.util.AppUiConstants).FALSE" manifestObj="true" maxValue="260.0" minValue="130.0" toolTipText="The requested USB PLL frequency internally configures the USB PLL dividers: NDIV and PDIV." value="192" widgetName="gfloat_usbplldesfreq" x:Style="BORDER">
							<p1:GFloat.layoutData>
								<GridData horizontalAlignment="LEFT" widthHint="50"/>
							</p1:GFloat.layoutData>
						</p1:GFloat>
						<p1:GLabel manifestObj="true" text="Actual USB PLL frequency [MHz]:" widgetName="glabel_usbpllactfreq">
							<p1:GLabel.layoutData>
								<GridData widthHint="180" horizontalAlignment="RIGHT"/>
							</p1:GLabel.layoutData>
						</p1:GLabel>
						<p1:GFloat description="The actual PLL frequency internally configures the PLL dividers: &#13;&#10;&lt;B&gt;NDIV, PDIV, K2DIV and K1DIV:&lt;/B&gt;&#13;&#10;&lt;OL&gt;&lt;LI&gt;In Normal Mode: fPLL is a scaled version of VCO Clock&lt;BR&gt;&#13;&#10;   &#9;&#9;&#9;&#9;&#9;&lt;B&gt;fPLL = (N * fSOC) / (P * K2) &lt;/B&gt;&lt;/LI&gt;&#13;&#10;&lt;LI&gt;In Prescaler Mode: fPLL is running at external frequency fOSC (bypassing a VCO)&lt;BR&gt;&#13;&#10;   &#9;&#9;&#9;&#9;&#9;&lt;B&gt;fPLL = fOSC / K1&lt;/B&gt;&lt;/LI&gt;&lt;/OL&gt;" enabled="false" format="*,6" mandatory="(com.ifx.davex.ui.controls.util.AppUiConstants).FALSE" manifestObj="true" maxValue="260.0" minValue="130.0" value="192" widgetName="gfloat_usbpllactfreq" x:Style="BORDER">
							<p1:GFloat.layoutData>
								<GridData horizontalAlignment="LEFT" widthHint="46"/>
							</p1:GFloat.layoutData>
						</p1:GFloat>
					</p1:GGroup>
					<p1:GMessage isError="false" manifestObj="true" text="Actual PLL frequency (fPLL) value is not as expected." widgetName="gmessage_clock_generation">
