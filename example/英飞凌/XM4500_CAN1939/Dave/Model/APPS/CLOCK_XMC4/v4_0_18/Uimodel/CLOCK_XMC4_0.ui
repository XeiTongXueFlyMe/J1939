		<p1:GTab text="Standby Clock Generation Settings" visible="true" enabled="true" widgetName="gtab_standbyclkgeneration">
			<p1:GTab.control>
				<p1:Tab widgetName="composite11">
					<p1:Tab.layout>
						<GridLayout/>
					</p1:Tab.layout>
					<p1:GGroup manifestObj="true" text="Standby Clock (fSTDBY)" widgetName="ggroup_standbyclkfreq">
						<p1:GGroup.layoutData>
							<GridData widthHint="716" horizontalAlignment="FILL"/>
						</p1:GGroup.layoutData>
						<p1:GGroup.layout>
							<GridLayout numColumns="2"/>
						</p1:GGroup.layout>
						<p1:GLabel text="Clock source:" widgetName="glabel_standbyclksrc">
							<p1:GLabel.layoutData>
								<GridData horizontalAlignment="RIGHT"/>
							</p1:GLabel.layoutData>
						</p1:GLabel>
						<p1:GCombo description="The standby clock of 32.768kHz is generated in hibernate domain:&#13;&#10;&lt;OL&gt;&lt;LI&gt;External Crystal Ultra Low Power Oscillator: Precise and stable clock is propagated to Core domain as fSTDBY and used for continuos adjustments the fast internal backup clock source fOFI.&lt;/LI&gt;&#13;&#10;&lt;LI&gt; Internal Slow Oscillator: Used for independent clock source for WDT module and as the clock for periodic wakeup events in power saving modes, and for continuos adjustments the fast internal backup clock source fOFI.&lt;/LI&gt;&#13;&#10;&lt;/OL&gt;" manifestObj="true" options="External Crystal Ultra Low Power Oscillator&#13;&#10;Internal Slow Oscillator" toolTipText="Select a source of standby clock." value="Internal Slow Oscillator" widgetName="gcombo_standbyclksrc">
							<p1:GCombo.layoutData>
